<Window x:Name="EnterPassword1" x:Class="CloudCoin_SafeScan.EnterPassword"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CloudCoin_SafeScan"
        mc:Ignorable="d"
        Title="Enter Password for Safe" Height="128.956" Width="300" ResizeMode="NoResize">
    <Grid Height="59" VerticalAlignment="Top">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="89*"/>
            <ColumnDefinition Width="57*"/>
        </Grid.ColumnDefinitions>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Height="29" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" Grid.ColumnSpan="2"/>
        <Button x:Name="OKbutton" Content="OK" HorizontalAlignment="Left" Height="27" Margin="74,55,0,-23" VerticalAlignment="Top" Width="141" Grid.ColumnSpan="2" Click="onOKClick"/>

    </Grid>
</Window>
