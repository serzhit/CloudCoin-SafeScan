<Window x:Name="EnterPassword1" x:Class="CloudCoin_SafeScan.EnterPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CloudCoin_SafeScan"
        xmlns:resx="clr-namespace:CloudCoin_SafeScan.Properties"
        mc:Ignorable="d"
        SizeToContent="Height"
        Title="{x:Static resx:Resources.EnterSafePassword}" Height="auto" Width="300" ResizeMode="NoResize">
    <StackPanel VerticalAlignment="Top">
        <Label x:Name="PwdLabel" Content="Enter Password:" Height="auto" Width="auto" Margin="10,10,10,0" />
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Height="29" Margin="10,0,10,0" VerticalAlignment="Top" Width="272" />
        <Button x:Name="OKbutton" Content="OK" HorizontalAlignment="Center" Height="27" Margin="0,20,0,20" VerticalAlignment="Top" Width="141" Click="onOKClick" IsDefault="True"/>
    </StackPanel>
</Window>
