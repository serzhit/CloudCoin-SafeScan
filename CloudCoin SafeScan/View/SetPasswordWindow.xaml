<Window x:Class="CloudCoin_SafeScan.SetPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CloudCoin_SafeScan"
        xmlns:resx="clr-namespace:CloudCoin_SafeScan.Properties"
        mc:Ignorable="d"
        Title="{x:Static resx:Resources.SetPasswordWindowTitle}" Height="311.968" Width="314.809">
    <Grid Margin="0,0,0,200">
        <Label x:Name="label" Content="{x:Static resx:Resources.SeemsNotSetPassword}" HorizontalAlignment="Left" Height="112" Margin="15,10,0,-65" VerticalAlignment="Top" Width="282"/>
        <PasswordBox x:Name="Password" HorizontalAlignment="Left" Height="30" Margin="15,128,0,-101" VerticalAlignment="Top" Width="282" AutomationProperties.HelpText="Enter password" ToolTip="Enter Password" TextInput="onTextInput"/>
        <PasswordBox x:Name="PasswordVerify" HorizontalAlignment="Left" Height="30" Margin="15,163,0,-136" VerticalAlignment="Top" Width="282" ToolTip="Verify password" AutomationProperties.HelpText="Verify password" TextInput="onVerifyTextInput"/>
        <Button x:Name="buttonOK" Content="{x:Static resx:Resources.Ok}" Height="22" Margin="72,213,72,-178" VerticalAlignment="Top" IsDefault="True" Click="okButtonClicked"/>
    </Grid>
</Window>
